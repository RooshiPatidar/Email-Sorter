<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PrimaryScreenController">
   <children>
      <VBox AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <ImageView fx:id="addFolderImageView" fitHeight="85.0" fitWidth="109.0" onMouseClicked="#goToAddNewFolder" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../res/images/succ.jpg" />
                     </image></ImageView>
                  <ImageView fx:id="editFolderImageView" fitHeight="85.0" fitWidth="109.0" layoutX="10.0" layoutY="10.0" onMouseClicked="#goToEditFolder" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../res/images/IMG_20160508_180126_01.png" />
                     </image></ImageView>
                  <ImageView fx:id="removeFolderImageView" fitHeight="85.0" fitWidth="109.0" layoutX="119.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../res/images/sooraj.png" />
                     </image></ImageView>
                  <ImageView fx:id="preferencesImageView" fitHeight="85.0" fitWidth="109.0" layoutX="228.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../res/images/IMG_20160519_101805_01.jpg" />
                     </image></ImageView>
               </children></HBox>
            <ScrollPane fitToWidth="true" prefHeight="305.0" prefWidth="600.0">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="305.0" prefWidth="200.0" />
              </content>
            </ScrollPane>
         </children></VBox>
   </children>
</AnchorPane>
